{
    "name": "EAVA (Extend AVA)",
    "version": "1.2",
    "description": "",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://ava.cefor.ifes.edu.br/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://ava.cefor.ifes.edu.br/*"
            ],
            "js": [
                "script.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}
